<h1 class="text-center">Photo一覧</h1>
<div class="container">
	<div class="row">
		<div class="col-lg-5">
		</div>
		<div class="col-lg-6">
			<% @photos.each do |photo| %>
				<%= link_to photo_path(photo) do %>
					<%= attachment_image_tag blog, :image, size: "190x150" %>
				<% end %>
				<p>
					<%= photo.title %>
				</p>
			<% end %>
		</div>
	</div>
</div>